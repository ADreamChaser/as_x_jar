        </activity>
        <activity
            android:name="com.litepal.instance.activity.AverageSampleActivity"
            android:label="@string/average_sample"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.litepal.instance.activity.SumSampleActivity"
            android:label="@string/sum_sample"
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>
