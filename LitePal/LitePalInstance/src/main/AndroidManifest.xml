<manifest
    package="com.litepal.instance"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name="com.litepal.instance.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.litepal.instance.activity.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="com.litepal.instance.activity.ManageTablesActivity"
            android:label="@string/manage_table_sample"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.litepal.instance.activity.ModelListActivity"
            android:label="@string/manage_table_sample"
