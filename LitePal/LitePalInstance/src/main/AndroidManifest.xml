        </activity>
        <activity
            android:name="com.litepal.instance.activity.TableListActivity"
            android:label="@string/manage_table_sample"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.litepal.instance.activity.ModelStructureActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.litepal.instance.activity.TableStructureActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.litepal.instance.activity.CRUDActivity"
            android:label="@string/crud_sample"
